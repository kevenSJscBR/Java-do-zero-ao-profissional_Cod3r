package oo.heranca.desafio;

public class CarroTeste {
	
	public static void main(String[] args) {
		
		Carro c = new Civic();
		
		c.acelerar();
		System.out.println(c);
		
		c.acelerar();
		System.out.println(c);
		
		c.acelerar();
		System.out.println(c);
		
		System.out.println("-=-=-=-=-=-=-=-=-=-=-");
		
		Ferrari c2 = new Ferrari();
		c2.acelerar();
		c2.frear();
		System.out.println(c2);
		
		c2.acelerar();
		c2.frear();
		System.out.println(c2);
		
		c2.acelerar();
		System.out.println(c2);
		
	}

}
